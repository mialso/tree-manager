{
    "name": "tree_manager",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "webpack --env dev",
        "build:prod": "webpack --env prod",
        "compile": "swc ./src --config-file .swcrc -d ./dist",
        "exec": "node build/bundle.js",
        "exec:debug": "node --inspect-brk build/bundle.js",
        "dep:graph": "npx madge --image graph.svg src/",
        "lint": "npx eslint src/",
        "test": "echo \"Error: no test specified\" && exit 1",
        "postinstall": "npm run compile"
    },
    "type": "module",
    "exports": {
        "./*": "./dist/src/*"
    },
    "author": "mialso",
    "license": "ISC",
    "peerDependencies": {
        "react-reconciler": "0.29.2",
        "react": "18.3.1"
    },
    "dependencies": {
        "redux": "^5.0.1",
        "tsee": "^1.3.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.1.1",
        "@swc/cli": "^0.7.8",
        "@swc/core": "^1.13.3",
        "@types/react": "18.3.2",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "madge": "3.9.2",
        "ts-loader": "9.5.1",
        "typescript": "5.4.5",
        "typescript-eslint": "^7.7.0",
        "webpack": "5.91.0",
        "webpack-cli": "5.1.4"
    },
    "madge": {
        "fileExtensions": [
            "ts",
            "tsx"
        ],
        "tsConfig": "./tsconfig.json"
    }
}
